<!DOCTYPE html>
<html>
<head>
  <title>dJOBi-dJOBa</title>
 
  <%= stylesheet_link_tag 'application'%>
  
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
<link href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.0/themes/base/jquery-ui.css" rel="stylesheet" type="text/css" />
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js"></script>
<script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.0/jquery-ui.min.js"></script>
 <link href="//cdnjs.cloudflare.com/ajax/libs/select2/4.0.0/css/select2.min.css" rel="stylesheet" />
<script src="//cdnjs.cloudflare.com/ajax/libs/select2/4.0.0/js/select2.min.js"></script>
</head>
<body>
	<%= render 'layouts/header' %>
    <div class="container container-full">
      <% flash.each do |message_type, message| %>
        <div class="alert alert-<%= message_type == 'notice' ? 'info' : message_type %>"><%= message %></div>
      <% end %>
      <%= yield %>
    </div>
</body>
</html>
