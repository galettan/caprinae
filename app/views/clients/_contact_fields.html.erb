	<table class="table table-striped table-bordered">
		<tr>
			<td>
				<%= f.label :prenom%>
			</td>
			<td>
				<%= f.text_field :first_name, class: "form-control"%>
			</td>
		</tr>
		<tr>
			<td>
				<%= f.label :nom%>
			</td>
			<td>
				<%= f.text_field :last_name, class: "form-control"%>
			</td>
		</tr>
		<tr>
			<td>
				<%= f.label :email%>
			</td>
			<td>
				<%= f.email_field :email, class: "form-control"%>
			</td>
		</tr>
		<tr>
			<td>
				<%= f.label :telephone%>
			</td>
			<td>
				<%= f.text_field :phone_nbr, class: "form-control"%>
			</td>
		</tr>
		</table>
		<p class="text-left">
		<%= link_to_remove_association "Retirer le contact", f, class: "btn btn-danger" %>
		</p>
