<p><%= link_to 'Nouveau Projet', new_project_path, class: 'btn btn-default'%>
</p><div class="panel panel-default">
	<div class="panel-heading">
		<strong>Liste des Projets</strong>
	</div>
	<div class="panel-body">
		<table class="table table-bordered text-center table-hover table-striped">
			<thead>
				<tr>
					<th>#</th>
					<th>Client</th>
					<th>Nom du projet</th>
					<th>Type de projet</th>
					<th>Priorité</th>
					<th>Asssigné à</th>
					<th>Temps estimé / réel</th>
					<th>Date premier retour</th>
					<th>Date de validation</th>
					<th>Bon à tirer</th>
					<th>nombre de retour estimé / réel</th>
					<th>Statut</th>
				</tr>
			</thead>
			<tbody>
			<% @projects.each do |project| %>
				<%= render project%>
			<% end %>
			</tbody>
		</table>
		<div class="text-center">
			<%= will_paginate %>
		</div>
	</div>
</div>	
