<div role="tabpanel" class="tab-pane <%= current_user.print? ? 'active' : ''%>" id="print">
	<div class="clearfix">
		<div class="panel panel-default">
			<div class='panel-heading'>
				<strong>Informations Print :</strong>
			</div>
			<div class="panel-body">
				<table class="table table-bordered table-hover table-striped">
					<tr>
						<td>Bon à tirer</td>
						<td><%= t @project.test_print%></td>
					</tr>
					<tr>
						<td>Couleur</td>
						<td><%= t @project.colors %></td>
					</tr>
					<tr>
						<td>Données variables</td>
						<td><%= @project.various_input? ? 'oui' : 'non'%></td>
					</tr>
					<tr>
						<td>Quantité bonnes feuilles</td>
						<td><%= @project.good_sheets_qty%></td>
					</tr>
					<tr>
						<td>Quantité documents finis</td>
						<td><%= @project.finished_doc_qty%></td>
					</tr>
					<tr>
						<td>Laisser brut de tirage</td>
						<td><%= @project.let_raw? ? 'oui' : 'non'%></td>
					</tr>
					<tr>
						<td>Format ouvert (mm)</td>
						<td><%= t @project.open_shape %></td>
					</tr>
					<tr>
						<td>Format Fini</td>
						<td><%= t @project.finished_shape %></td>
					</tr>
					<tr>
						<td>Pelliculage ou plastification</td>
						<td><%= t @project.filming %></td>
					</tr>
					<tr>
						<td>Façonnage</td>
						<td><%= t @project.shaping%></td>
					</tr>
					<tr>
						<td>Paqutage</td>
						<td><%= t @project.package%></td>
					</tr>
					<tr>
						<td>Livraison</td>
						<td><%= @project.carrier.nil? ? '' : @project.carrier.name%></td>
					</tr>
					<% if !@project.tracking_nbr.empty?%>
					<tr>
						<td>Numéro de suivi</td>
						<td><%= t @project.tracking_nbr%></td>
					</tr>
					<% end%>
					<tr>
						<td>Nombre de colis</td>
						<td><%= t @project.package_nbr.nil? ? 0 : @project.package_nbr%></td>
					</tr>
					<tr>
						<td>Adresse de livraison (si différente de fiche client)</td>
						<td><%= @project.delivery_address.nil? ? '' : @project.delivery_address%></td>
					</tr>
					<tr>
						<td>Remarques</td>
						<td><%= @project.notice%></td>
					</tr>
					<tr>
						<td>Sous traitance</td>
						<td><%= @project.outsourcing%></td>
					</tr>
				</table>
			</div>
		</div>
		<div class="panel panel-info">
			<div class="panel-heading">
				<strong>Papier(s)</strong>
			</div>
			<div class="panel-body">
				<table class="table table-striped table-hover table-bordered">
					<thead>
						<th>Type</th>
						<th>Grammage</th>
						<th>Format</th>
					</thead>
					<tbody>
						<% @project.papers.each do |paper| %>
						<tr>
							<td><%= t paper.paper %></td>
							<td><%= t paper.density%></td>
							<td><%= t paper.shape%></td>
						</tr>
						<% end %>
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>
